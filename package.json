{
  "name": "money-cracker",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --ios --offline --go",
    "start:online": "expo start --ios --go",
    "start:dev-build": "expo start --ios --dev-client",
    "build:ios": "eas build --local --platform ios",
    "build:ios-simulator": "bun build:pre && eas build -p ios --profile development --local --output dist/build.tar.gz && bun build:unpack",
    "build:pre": "rm -rf dist && mkdir dist",
    "build:unpack": "tar -xzf ./dist/build.tar.gz -C dist/ && rm ./dist/build.tar.gz",
    "db:new-migration": "drizzle-kit generate",
    "update-deps": "npm-check-updates -x chalk && bun install",
    "postinstall": "./scripts/post_install.sh",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "@babel/core": "^7.26.10",
    "@eva-design/eva": "^2.2.0",
    "@expo/react-native-action-sheet": "^4.1.1",
    "@floating-ui/react-native": "^0.10.7",
    "@react-native-community/datetimepicker": "8.2.0",
    "@shopify/flash-list": "1.7.3",
    "@shopify/react-native-skia": "1.5.0",
    "@tanstack/react-form": "^1.1.2",
    "@tanstack/react-query": "^5.69.0",
    "@types/lodash-es": "^4.17.12",
    "@types/react": "~18.3.12",
    "@ui-kitten/components": "^5.3.1",
    "@ui-kitten/eva-icons": "^5.3.1",
    "@ui-kitten/metro-config": "^5.3.1",
    "@valibot/to-json-schema": "^1.0.0",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-plugin-react-compiler": "^19.0.0-beta-aeaed83-20250323",
    "date-fns": "^4.1.0",
    "drizzle-kit": "^0.30.5",
    "drizzle-orm": "^0.41.0",
    "eas-cli": "^16.0.1",
    "expo": "~52.0.40",
    "expo-crypto": "~14.0.2",
    "expo-dev-client": "~5.0.15",
    "expo-file-system": "~18.0.12",
    "expo-image": "~2.0.6",
    "expo-image-picker": "~16.0.6",
    "expo-local-authentication": "~15.0.2",
    "expo-localization": "~16.0.1",
    "expo-router": "~4.0.19",
    "expo-secure-store": "~14.0.1",
    "expo-splash-screen": "~0.29.22",
    "expo-sqlite": "~15.1.2",
    "expo-status-bar": "~2.0.1",
    "expo-system-ui": "~4.0.8",
    "i18next": "^24.2.3",
    "lodash-es": "^4.17.21",
    "react": "18.3.1",
    "react-compiler-runtime": "^19.0.0-beta-aeaed83-20250323",
    "react-i18next": "^15.4.1",
    "react-native": "0.76.7",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "15.8.0",
    "typescript": "^5.8.2",
    "valibot": "^1.0.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@stylistic/eslint-plugin": "^4.2.0",
    "chalk": "4.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-compiler": "^19.0.0-beta-aeaed83-20250323",
    "globals": "^16.0.0",
    "lint-staged": "^15.5.0",
    "npm-check-updates": "^17.1.16",
    "typescript-eslint": "^8.28.0"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  }
}
